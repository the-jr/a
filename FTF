coroutine.wrap(XMAJ_fake_script)()
local function XEHLGIT_fake_script() -- Noclip.NoclipScript 
	local script = Instance.new('LocalScript', Noclip)

	pl = game.Players.LocalPlayer
	char = pl.Character
	
	on = false
	
	storagefold = script.Parent.Parent.Parent.Storage
	thing = storagefold:WaitForChild("BypassChecker", 1)
	
	function res()
		game:GetService("RunService").Stepped:Connect(function()
			for i, v in pairs(char:GetChildren()) do
				if v:IsA("BasePart") and on == true then
					if thing.Value then
						v.CanCollide = false
					else
						script.Parent.Text = "NB-Noclip: OFF"
						on = false
						warn("Bypass needed to work for noclip!")
						break
					end
				elseif on == false then
					break
				end
			end
		end)
	end
	
	script.Parent.MouseButton1Up:Connect(function()
		if on == false then
			on = true
			script.Parent.Text = "NB-Noclip: ON"
			res()
		else
			on = false
			script.Parent.Text = "NB-Noclip: OFF"
		end
	end)
end
coroutine.wrap(XEHLGIT_fake_script)()
local function VAUJII_fake_script() -- InfJump.LocalScript 
	local script = Instance.new('LocalScript', InfJump)

	infon = false
	
	function therealfunction()
		while task.wait() do
			if infon == true then
				if game.Players.LocalPlayer.Character.Humanoid.Jump == true then
					if script.Parent.Text ~= "InfJump: ON" then script.Parent.Text = "InfJump: ON" end
					local part = Instance.new("Part")
					part.Transparency = 1
					part.Parent = game.Players.LocalPlayer.Character["HumanoidRootPart"]
					part.Position = game.Players.LocalPlayer.Character["HumanoidRootPart"].Position + Vector3.new(0, -3.575, 0)
					task.wait()
					part:Destroy()
				end
			else
				script.Parent.Text = "InfJump: OFF"
				break
			end
		end
	end
	
	script.Parent.MouseButton1Up:Connect(function()
		infon = not infon
		therealfunction()
	end)
end
coroutine.wrap(VAUJII_fake_script)()
local function WYTDZY_fake_script() -- WS.LocalScript 
	local script = Instance.new('LocalScript', WS)

	function loop()
		local hum = game.Players.LocalPlayer.Character.Humanoid
		while task.wait() do
			if script.Parent.Visible == true or script.Parent.Parent.Keep.Text == "Loop WS: ON" then
				hum.WalkSpeed = script.Parent.Text
			elseif script.Parent.Visible == false and script.Parent.Parent.Keep.Text ~= "Loop WS: ON" then
				spawn(function()
					loop()
				end)
				break
			end
		end
	end
	
	while task.wait(1) do
		if script.Parent.Visible then
			loop()
		end
	end
end
coroutine.wrap(WYTDZY_fake_script)()
