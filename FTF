-- Scripts:

local function XKNUSET_fake_script() -- Main.Drag 
	local script = Instance.new('LocalScript', Main)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(XKNUSET_fake_script)()
local function IZINBX_fake_script() -- Exit.ExitScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Up:Connect(function()
		script.Parent.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(IZINBX_fake_script)()
local function TBGVUH_fake_script() -- Minimize.Minimize 
	local script = Instance.new('LocalScript', Minimize)

	t = script.Parent.Parent
	mainq = t.Parent
	button = script.Parent
	fold = script.Parent.Parent.Parent.Buttons
	onmini = false
	
	button.MouseButton1Up:Connect(function()
		if onmini == false then
			onmini = true
			t.Position = UDim2.fromScale(0, 0.9)
			mainq.BackgroundTransparency = 1
			t.Label.Transparency = 1
			button.Text = "+"
			for i, v in pairs(fold:GetChildren()) do
				if v ~= nil then
					v.Visible = false
				end
			end
		else
			onmini = false
			t.Position = UDim2.fromScale(0, 0)
			mainq.BackgroundTransparency = 0
			t.Label.Transparency = 0
			button.Text = "-"
			for i, v in pairs(fold:GetChildren()) do
				if v ~= nil and v.ClassName ~= "TextBox" then
					v.Visible = true
				end
			end
		end
	end)
end
coroutine.wrap(TBGVUH_fake_script)()
local function GSUGULN_fake_script() -- ESP.ESPScript 
	local script = Instance.new('LocalScript', ESP)

	storage = script.Parent.Parent.Parent.Storage
	bhighlight = Instance.new("Highlight")
	bhighlight.Parent = storage
	bhighlight.FillColor = Color3.fromRGB(255, 0, 0)
	bhighlight.OutlineColor = Color3.fromRGB(255, 0, 0)
	bhighlight.Name = "BeastHighlight"
	
	phighlight = Instance.new("Highlight")
	phighlight.Parent = storage
	phighlight.FillColor = Color3.fromRGB(255, 255, 255)
	phighlight.Name = "PlayerHighlight"
	
	chighlight = Instance.new("Highlight")
	chighlight.Parent = storage
	chighlight.FillColor = Color3.fromRGB(0, 150, 255)
	chighlight.OutlineColor = Color3.fromRGB(0, 255, 255)
	chighlight.Name = "ComputerHighlight"
	game.Players.RespawnTime = 0
	
	onq = false
	
	function plr()
		for i, v in pairs(game.Players:GetPlayers()) do
			if onq == true then
				storage.PlayerHighlight:Clone().Parent = v.Character
				for i, scr in pairs(v.Character:GetChildren()) do
					if scr:IsA("LocalScript") and scr.Name == "PowersLocalScript" then
						remove(v.Character)
						storage.BeastHighlight:Clone().Parent = v.Character
					end
				end
			else
				remove(v.Character)
			end
		end
	end
	
	function remove(thing)
		for i, v in pairs(thing:GetChildren()) do
			if v:IsA("Highlight") then
				v:Destroy()
			elseif v:IsA("BasePart") or v:IsA("Accessory") then
				remove(v)
			end
		end
	end
	
	function main()
		--computer
		for i, v in pairs(game.Workspace:GetDescendants()) do
			if v ~= nil and v:IsA("Model") and v.Name == "ComputerTable" then
				if onq == true then
					storage.ComputerHighlight:Clone().Parent = v
				else
					remove(v)
				end
			end
		end
		--players + beast
		plr()
	end
	
	debounce = 0
	
	script.Parent.MouseButton1Up:Connect(function()
		if debounce == 0 then
			debounce = 1
			spawn(function()
				if onq == false then
					onq = true
					script.Parent.Text = "ESP: ON"
					main()
				else
					onq = false
					script.Parent.Text = "ESP: OFF"
					main()
				end
			end)
			task.wait(0.25)
			debounce = 0
		end
	end)
end
coroutine.wrap(GSUGULN_fake_script)()
