local function BBYCWFR_fake_script() -- Keep.LocalScript 
	local script = Instance.new('LocalScript', Keep)

	val = false
	
	script.Parent.MouseButton1Up:Connect(function()
		val = not val
		if val == true then
			script.Parent.Text = "Loop WS: ON"
		else
			script.Parent.Text = "Loop WS: OFF"
		end
	end)
end
coroutine.wrap(BBYCWFR_fake_script)()
local function LWVBEX_fake_script() -- BypassAnti.LocalScript 
	local script = Instance.new('LocalScript', BypassAnti)

	bool = Instance.new("BoolValue", script.Parent.Parent.Parent.Storage)
	bool.Name = "BypassChecker"
	bool.Value = false
	
	script.Parent.MouseButton1Up:Connect(function()
		task.wait(.1)
		char = game.Players.LocalPlayer.Character
		root = char.HumanoidRootPart
		root:Destroy()
		newroot = Instance.new("Part")
		newroot.Name = "HumanoidRootPart"
		newroot.CFrame = char.Torso.CFrame
		newroot.Size = Vector3.new(2,2,1)
		newroot.Transparency = 1
		newroot.Parent = char
		newroot.Color = Color3.fromRGB(163,162,165)
		attach = Instance.new("Attachment", newroot)
		attach.Name = "RootAttachment"
		joint = Instance.new("Motor6D", newroot)
		joint.Part0 = newroot
		joint.Part1 = char.Torso
		bool.Value = true
	end)
end
coroutine.wrap(LWVBEX_fake_script)()
local function QEPZEME_fake_script() -- ToRandom.LocalScript 
	local script = Instance.new('LocalScript', ToRandom)

	storagefold = script.Parent.Parent.Parent.Storage
	thing = storagefold:WaitForChild("BypassChecker", 1)
	
	function dothething()
		task.wait(.1)
		local root = game.Players.LocalPlayer.Character.HumanoidRootPart
		local amount = #game.Players:GetPlayers()
		if amount == 1 then return end
		local numbuh = math.random(1, #game.Players:GetPlayers())
		local nameofpl = game.Players:GetPlayers()[numbuh]
		if nameofpl.Name ~= game.Players.LocalPlayer.Name then
			root.CFrame = nameofpl.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 5)
		else
			dothething()
		end
	end
	
	script.Parent.MouseButton1Up:Connect(function()
		if thing.Value then
			dothething()
		else
			warn("Bypass needed to work for teleportation!")
		end
	end)
end
coroutine.wrap(QEPZEME_fake_script)()
local function DOWAU_fake_script() -- Rejoin.LocalScript 
	local script = Instance.new('LocalScript', Rejoin)

	script.Parent.MouseButton1Up:Connect(function()
		game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
	end)
end
coroutine.wrap(DOWAU_fake_script)()
